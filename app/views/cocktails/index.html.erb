<h1> Experimental Cocktail List </h1>
<h4><%= link_to "create a new cocktail", new_cocktail_path %></h4>
<ol>
  <% @cocktails.each do |cocktail| %>
    <li>
      <p><%= cocktail.name %></p>
      <%= link_to "see ingredients list", cocktail_path(cocktail) %>
      <%= link_to "delete", cocktail_path(cocktail), method: :delete %>
    </li>
  <% end %>
</ol>
